{
  "short_name": "Boring Extension",
  "name": "Boring Extension",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Simple chrome extension that will make you productive by making it easier for you to navigate the web by upgrading your new tab page and extend functionalities of selected web apps",
  "icons": {
    "128": "rocket.png"
  },
  "content_security_policy": "script-src 'self' 'sha256-186Qn3EYHS1S1DwKJ3O3xRXnWe14LB2RMcMszp71nCQ='; object-src 'self'",
  "permissions": [
    "storage"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://app.slack.com/*"
      ],
      "js": [
        "contentScripts/slack.js"
      ]
    },
    {
      "matches": [
        "https://bitbucket.org/*pull-requests*"
      ],
      "js": [
        "contentScripts/bitbucket.js"
      ]
    }
  ]
}
